{
  "parser": "babel-eslint",
  "extends": "airbnb",
   "rules": {
     "object-curly-newline": 0,
     "no-return-assign": 0,
     "no-mixed-operators": 0,
     "computed-property-spacing": "warn",
     "no-plusplus": 0,
     "prefer-const": "warn",
     "space-in-parens": "warn",
     "keyword-spacing": "warn",
     "camelcase": 0,
     "import/first": 0,
     "import/no-extraneous-dependencies": 0,
     "import/prefer-default-export": 0,
     "class-methods-use-this": 0,
     "import/extensions": 0,
     "arrow-parens": 0,
     "key-spacing": "warn",
     "mocha/no-exclusive-tests": "error",
     "arrow-body-style": 0,
     "block-spacing": "warn",
     "brace-style": "warn",
     "comma-spacing": "warn",
     "comma-dangle": 0,
     "consistent-return": 0,
     "curly": ["warn", "multi-line"],
     "eol-last": 0,
     "func-names": 0,
     "import/no-unresolved": 0,
     "indent": "warn",
     "max-len": 0,
     "newline-per-chained-call": 0,
     "no-case-declarations": 0,
     "no-debugger": 0,
     "no-else-return": 0,
     "no-nested-ternary": 0,
     "no-param-reassign": 0,
     "no-restricted-syntax": 0,
     "no-trailing-spaces": 0,
     "no-underscore-dangle": 0,
     "no-unused-vars": "warn",
     "no-use-before-define": 0,
     "object-curly-spacing": "warn",
     "object-shorthand": "warn",
     "padded-blocks": 0,
     "prefer-arrow-callback": "warn",
     "prefer-template": "warn",
     "quotes": "warn",
     "radix": 0,
     "semi": 0,
     "space-before-function-paren": 0,
     "space-infix-ops": "warn",
     "template-curly-spacing": "warn",
     "no-fallthrough": 0,
     "new-cap": 0,
     "spaced-comment": "warn",
     "no-multi-spaces": "warn",
     "react/prop-types": 0,
   },
   "plugins": [
      "mocha"
    ],
   "env": {
     "es6": true,
     "browser": true
   },
   "globals": {
     "__IS_BROWSER__": true,
     "it": true,
     "describe": true,
     "afterEach": true,
     "afterAll": true,
     "beforeEach": true,
     "beforeAll": true,
     "PIXI": true,
     "expect": true,
     "test": true,
     "jest": true,
     "jasmine": true
   }
}

